{
    "name": "gpoehl/phpreport",
    "description": "PHP library to process data sets for applications like reports. Handles group changes, calculates totals and subtotals and joins several data sets.",
    "authors": [
        {
            "name": "Guenter Poehl"
        }
    ],
    "keywords": [
        "php",
        "report",
		"reporting", 
		"group",
        "grouping",
        "totals",
        "subtotals",
        "sheets" ,
        "grid table"
    ],
    "archive": {
        "exclude": ["/docs",  "/docs/build"]
    },
    "require": {
        "php": ">=8.0"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
