{
    "name": "gpoehl/phpreport",
    "description": "Backbone for reports or any batch like application. Manages group changes and provides aggregate functions. Integrates seemless into any environment.",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Guenter Poehl"
        }
    ],
    "keywords": [
        "php",
        "report",
        "grouping",
        "group change",
        "totals",
        "subtotals",
        "sheets"
    ],
    "archive": {
        "exclude": ["/docs",  "/docs/build"]
    },
    "require": {
        "php": "^7.3"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
